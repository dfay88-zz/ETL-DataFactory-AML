{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-aml-df"
		},
		"OnPremFileSystem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremFileSystem'"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"OnPremFileSystem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\dafay\\OneDrive - Microsoft\\Documents\\Demos\\ETL-DataFactory-AML\\Data"
		},
		"OnPremFileSystem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "NORTHAMERICA\\dafay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-aml-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OnPremFileSystem_password": {
						"value": "[parameters('OnPremFileSystem_password')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"OnPremFileSystem_properties_typeProperties_host": {
						"value": "[parameters('OnPremFileSystem_properties_typeProperties_host')]"
					},
					"OnPremFileSystem_properties_typeProperties_userId": {
						"value": "[parameters('OnPremFileSystem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}